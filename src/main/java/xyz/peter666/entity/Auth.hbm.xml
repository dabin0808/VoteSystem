<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    
 <hibernate-mapping package="xyz.peter666.entity">

 		
 	<class name="Auth" table="tab_auth">
		<id name="authId" column="authId">
			<generator class="native"></generator>
		</id>
		<property name="createTime" column="createTime" type="java.util.Date"></property>
		<property name="status" column="status"></property>
		<property name="userId" column="userId"></property>
		<property name="username" column="username"></property>
	</class>
 
 </hibernate-mapping>   