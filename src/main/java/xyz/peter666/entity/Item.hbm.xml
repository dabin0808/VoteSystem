<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    
 <hibernate-mapping package="xyz.peter666.entity">

 		
 	<class name="Item" table="tab_item">
		<id name="itemId" column="itemId">
			<generator class="native"></generator>
		</id>
		<property name="optionId" column="optionId"></property>
		<property name="subjectId" column="subjectId"></property>
		<property name="userId" column="userId"></property>
		<property name="time" column="time"></property>
	</class>
 
 </hibernate-mapping>   